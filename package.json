{
  "name": "tester-chrome-extension",
  "version": "2.0.1",
  "description": "The Rainforest Chrome Extension for Testers (internal JS)",
  "main": "src/index.js",
  "scripts": {
    "test": "mocha --require jsdom-global/register --compilers js:babel-register src/\\*\\*/__tests__/\\*.js",
    "test:ci": "nyc --require babel-register npm test -- --reporter mocha-junit-reporter",
    "coveralls": "nyc report --reporter=text-lcov | coveralls",
    "lint": "eslint src",
    "lint:fix": "eslint --fix src",
    "build:browser": "mkdir -p build && browserify src/bootstrap.js -d -o build/main.js -t [ babelify --presets [ es2015 ] ] -p [ minifyify --map main.js.map --output build/main.js.map ]",
    "build": "npm run build:browser",
    "watch": "mkdir -p build && watchify src/bootstrap.js -d -o build/main.js -t [ babelify ]",
    "devServer": "cd build && ws -p 8123 -r '/DEV/* -> /$1'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rainforestapp/tester-chrome-extension.git"
  },
  "keywords": [
    "rainforest",
    "chrome-extension"
  ],
  "author": "Russell Smith",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rainforestapp/tester-chrome-extension/issues"
  },
  "homepage": "https://github.com/rainforestapp/tester-chrome-extension#readme",
  "devDependencies": {
    "babel": "6.5.2",
    "babel-cli": "6.5.1",
    "babel-core": "6.5.2",
    "babel-eslint": "6.0.4",
    "babel-preset-es2015": "6.5.0",
    "babel-register": "6.11.6",
    "babelify": "7.2.0",
    "browserify": "13.0.0",
    "chai": "3.5.0",
    "chai-immutable": "1.6.0",
    "coveralls": "2.11.12",
    "eslint": "3.1.1",
    "eslint-config-rainforest": "0.1.2",
    "eslint-plugin-babel": "2.2.0",
    "eslint-plugin-flow-vars": "0.5.0",
    "eslint-plugin-import": "1.12.0",
    "eslint-plugin-jsx-a11y": "2.0.1",
    "eslint-plugin-react": "5.2.2",
    "fetch-mock": "5.1.1",
    "jsdom": "9.4.1",
    "jsdom-global": "2.0.0",
    "local-web-server": "1.2.6",
    "minifyify": "7.3.3",
    "mocha": "2.5.3",
    "mocha-junit-reporter": "1.12.0",
    "nyc": "8.1.0",
    "sinon": "1.17.5",
    "sinon-chai": "2.8.0",
    "watchify": "3.7.0"
  },
  "dependencies": {
    "deep-freeze": "0.0.1",
    "immutable": "3.8.1",
    "phoenix": "1.2.0",
    "raven-js": "3.7.0",
    "redux": "3.5.2",
    "redux-actions": "0.10.1"
  }
}
